{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "@typescript-eslint/naming-convention": [
            "warn",
            {
                "selector": [
                    "variable",
                    "function",
                    "property"
                ],
                "format": [
                    "camelCase"
                ],
                "leadingUnderscore": "allow"
            }
        ]
    }
}